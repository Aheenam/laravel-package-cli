{
  "name": "aheenam/laravel-package-cli",
  "description": "A small CLI to scaffold packages for Laravel framework",
  "type": "composer-plugin",
  "require": {
    "php": "^7.0",
    "symfony/console": "^3.3",
    "symfony/var-dumper": "^3.3",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "league/flysystem-memory": "^1.0",
    "spatie/phpunit-snapshot-assertions": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Aheenam\\LaravelPackageCli\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aheenam\\LaravelPackageCli\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "bin": [
    "laravel-package"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rathes Sachchithananthan",
      "email": "rathes@aheenam.com"
    }
  ],
  "minimum-stability": "stable"
}
